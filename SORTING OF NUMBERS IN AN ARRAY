public class main {
    public static void main(String[]args){
        int [] arr={45,23,2,25};
        for(int j=0;j<3;j++) {
            for (int i = 0; i < 3; i++) {
                if (arr[i + 1] < arr[i]) {
                    int temp = arr[i];
                    arr[i] = arr[i + 1];
                    arr[i + 1] = temp;
                }
            }
        }
        for(int a:arr) System.out.println(a+"");
    }
}
